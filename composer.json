{
    "name": "sinapsteknologi/laravel-gdrive-storage",
    "description": "Laravel filesystem disk for Google Drive (Shared Drive & Service Account support)",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sinaps Teknologi",
            "email": "package+laravel-gdrive-storage@sinapsteknologi.id"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "google/apiclient": "^2.15"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "SinapsTeknologi\\GDriveStorage\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SinapsTeknologi\\GDriveStorage\\GDriveStorageServiceProvider"
            ],
            "aliases": {
                "GDriveStorage": "SinapsTeknologi\\GDriveStorage\\Facades\\GDriveStorage"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
